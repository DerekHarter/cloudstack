network:
  version: 2
  renderer: networkd
  ethernets:
    {{ ansible_default_ipv4.interface }}:
      dhcp4: false
      dhcp6: false
      optional: true
      mtu: 1500
  vlans:
    {{ ansible_default_ipv4.interface }}.739:
      id: 739
      link: {{ ansible_default_ipv4.interface }}
      dhcp4: false
  bridges:
    cloudbr0:
      addresses: [{{ ansible_default_ipv4.address }}/24]
      routes:
      - to: default
        via: 192.168.0.1
      nameservers:
        addresses: [8.8.8.8,8.8.4.4]
      interfaces: [{{ ansible_default_ipv4.interface }}]
      dhcp4: false
      dhcp6: false
      parameters:
        stp: true
        forward-delay: 0
    cloudbr1:
      dhcp4: false
      interfaces: [{{ ansible_default_ipv4.interface }}.739]
